<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" style="-fx-background-color: orange;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.TransportCompanySystem.Client.Controllers.UserPackageDetailsController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="800.0" prefWidth="1000.0" spacing="40.0">
         <children>
            <Label text="Szczegóły Paczki" textFill="#bf1313">
               <font>
                  <Font name="System Bold Italic" size="30.0" />
               </font>
               <padding>
                  <Insets top="15.0" />
               </padding>
            </Label>
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="480.0" spacing="40.0">
                     <children>
                        <Label text="Dane Nadawcy" textFill="#bf1313">
                           <font>
                              <Font name="System Bold Italic" size="21.0" />
                           </font>
                        </Label>
                        <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
                           <children>
                              <Label text="Imię" textFill="#bf1313">
                                 <font>
                                    <Font size="21.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="senderName" editable="false" prefWidth="300.0" />
                           </children>
                        </HBox>
                        <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
                           <children>
                              <Label text="Nazwisko" textFill="#bf1313">
                                 <font>
                                    <Font size="21.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="senderSurname" editable="false" prefWidth="300.0" />
                           </children>
                        </HBox>
                        <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
                           <children>
                              <Label text="Adres" textFill="#bf1313">
                                 <font>
                                    <Font size="21.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="senderAddress" editable="false" prefWidth="300.0" />
                           </children>
                        </HBox>
                     </children>
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                  </VBox>
                  <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="480.0" spacing="40.0">
                     <children>
                        <Label text="Dane Odbiorcy" textFill="#bf1313">
                           <font>
                              <Font name="System Bold Italic" size="21.0" />
                           </font>
                        </Label>
                        <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
                           <children>
                              <Label text="Imię" textFill="#bf1313">
                                 <font>
                                    <Font size="21.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="recipientName" editable="false" prefWidth="300.0" />
                           </children>
                        </HBox>
                        <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
                           <children>
                              <Label text="Nazwisko" textFill="#bf1313">
                                 <font>
                                    <Font size="21.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="recipientSurname" editable="false" prefWidth="300.0" />
                           </children>
                        </HBox>
                        <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
                           <children>
                              <Label text="Adres" textFill="#bf1313">
                                 <font>
                                    <Font size="21.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="recipientAddress" editable="false" prefWidth="300.0" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="480.0" spacing="40.0">
                     <children>
                        <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
                           <children>
                              <Label text="Numer Przesyłki" textFill="#bf1313">
                                 <font>
                                    <Font size="21.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="packageID" editable="false" prefWidth="250.0" />
                           </children>
                        </HBox>
                        <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
                           <children>
                              <Label text="Rozmiar" textFill="#bf1313">
                                 <font>
                                    <Font size="21.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="packageSize" editable="false" prefWidth="300.0" />
                           </children>
                        </HBox>
                        <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
                           <children>
                              <Label text="Czy posiada reklamację" textFill="#bf1313">
                                 <font>
                                    <Font size="21.0" />
                                 </font>
                              </Label>
                              <CheckBox fx:id="complaint" mnemonicParsing="false" />
                           </children>
                        </HBox>
                        <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
                           <children>
                              <Label text="Koszt" textFill="#bf1313">
                                 <font>
                                    <Font size="21.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="price" editable="false" prefWidth="300.0" />
                           </children>
                        </HBox>
                        <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
                           <children>
                              <Label text="Metoda Płatności" textFill="#bf1313">
                                 <font>
                                    <Font size="21.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="paymentMethod" editable="false" prefWidth="250.0" />
                           </children>
                        </HBox>
                     </children>
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                  </VBox>
                  <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="480.0">
                     <children>
                        <Label text="Status" textFill="#bf1313">
                           <font>
                              <Font size="21.0" />
                           </font>
                        </Label>
                        <TextArea fx:id="status" editable="false" prefHeight="290.0" prefWidth="150.0" />
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                           <children>
                              <Label text="Waga [kg]" textFill="#bf1313">
                                 <font>
                                    <Font size="21.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="packageWeight" editable="false" prefWidth="250.0" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#back" text="Wróć">
               <font>
                  <Font size="21.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
